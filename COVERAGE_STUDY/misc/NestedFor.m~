function [Cov_Results, time_map] = NestedFor(data)

Min_cov_lat = zeros(data.Nb, data.Na, data.Ni, data.Nw);

for w = 1:data.Nw
    
    curr_walker = [data.Nsat(w), data.Norb, data.walk_phas];
    
    for i = 1:data.Ni
        
        curr_inc = data.inc(i);
        
        for beam = 1:data.Nb
            curr_beam = bw(beam);       % inclination [deg]
            
                for smax = 1:data.Na
                    curr_sma = semi_major_axes(smax);              % semi-major axis [km]
                    [YYY, T, THETA, ~, h_user] = const_orbits(curr_walker, curr_beam, curr_sma, curr_inc, timesteps, N_orbits, alt, perturb);
                    time_map = time_mapping(curr_walker, YYY, T, THETA, h_user, lon, lat, para);
                    [Cov_Results] = getMinCoverage(lon, lat, time_map, trashold, T);
                    Min_cov_lat(beam, smax, i, w) = Cov_Results.cov_mars;
                end
        end
    end
end

Cov_Results.Min_cov_lat = Min_cov_lat;